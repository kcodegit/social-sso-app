{
  "name": "social-sso-app",
  "version": "1.0.0",
  "author": "kohei",
  "license": "MIT",
  "scripts": {
    "prod": "node ./build/bin/www",
    "dev": "nodemon ./build/bin/www --ignore src/",
    "clean": "rm -rf build",
    "build": "babel src -d build --ignore src/node_modules --copy-files",
    "watch_build": "babel --watch src -d build --ignore src/node_modules --copy-files",
    "clean_build": "yarn clean && yarn build",
    "test": "NODE_ENV='development' mocha --recursive"
  },
  "dependencies": {
    "config": "^3.0.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "google-auth-library": "^3.1.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.0.1",
    "nodemon": "^1.18.10"
  }
}
